pipeline{
    agent {
        label 'master'
    }

    stages{
        stage('安卓程序源码同步') {
            steps {
                dir("AndroidSampleApp"){
                    git branch:'master', url:'git@github.com:13651440608/AndroidSampleApp.git'
                }
            }
        }
        
         stage('安卓编译打包') {
            steps {
                    cd AndroidSampleApp
                    @sh gradlew clean assembleDebug
             }
         }
   
    }
}
